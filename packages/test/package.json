{
  "name": "@rompt/test",
  "version": "1.0.2",
  "main": "build/index.js",
  "scripts": {
    "prebuild": "shx rm -f tsconfig.package.tsbuildinfo",
    "build": "tsc -p ./tsconfig.json",
    "clean": "shx rm -rf build pnpm-lock.yaml node_modules tsconfig.package.tsbuildinfo",
    "bump": "npm version patch --force",
    "test": "ts-mocha --paths test/**/*.spec.ts --timeout 50000"
  },
  "devDependencies": {
    "@rompt/types": "workspace:*",
    "@types/node": "^18.15.5",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@rompt/client": "workspace:*",
    "@rompt/common": "workspace:*",
    "dotenv": "^16.0.3",
    "rompt": "workspace:*"
  }
}
